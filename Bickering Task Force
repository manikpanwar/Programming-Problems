//NOT WORKING
#include <iostream>
using namespace std;

int dfs(int);
int k=0,m=0,n=0;
int graph[5000][5000];
int size = 0;	//Of taskforce
int visited[5000];
int force[5000];

int main() 
{
	cin>>n>>m>>k;
	int t1,t2;
	for(int i=0;i<4000;i++) 
	{
		visited[i]=0;
		force[i]=0;
		for(int j=0;j<4000;j++)
		{
			graph[i][j]=0;
		}
	}
	for(int i=1;i<=m;i++)
	{
		cin>>t1>>t2;
		graph[t1][t2]=1;
		graph[t2][t1]=1;
	}
	
	 for(int i=1;i<=n;i++)
	 {
	 
	    int t =dfs(i);
	    if(t)
	    force[i]=1;
	 }

	for(int i=1;i<=n;i++)
	size+=force[i];
	
	if(size>0)
	{
		cout<<"YES"<<endl<<size;
	}else
	{
		cout<<"NO";
	}

	return 0;
}

int dfs(int pos)
{
	int co = 0;
	if(!visited[pos])
	{
		visited[pos]=1;
		for(int i=1;i<=n;i++)
		{
			if(graph[pos][i]==1&&!visited[i])
			{
				int t = dfs(i);
				if(t==1)
				{
					co++;
				
				}
			}
			if(graph[pos][i]==1&&force[i]==1)
			{
				co++;
			}
		}
	}else if(force[pos]==1)
	{
		co=k;
		cout<<"A";
	}
	 if(co>=k)
	 return 1;
	 else 
	 return 0;
	
	
}
